<core:FragmentDefinition
	  xmlns="sap.m"
      xmlns:layout="sap.ui.layout"
      xmlns:core="sap.ui.core"
      xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
      xmlns:viz.data="sap.viz.ui5.data"
      xmlns:viz="sap.viz.ui5.controls">
      <layout:FixFlex id='chartFixFlexchart03' minFlexSize="500" busy="{chart03View>/busy}"
		  busyIndicatorDelay="{chart03View>/delay}">
      	<layout:flexContent>
      		
            <viz:Popover id="idPopOverchart03"></viz:Popover>
            <viz:VizFrame id="idVizFramechart03" uiConfig="{applicationSet:'fiori'}"
                height='100%' width="100%" vizType='donut'>
                <viz:dataset>
                    <viz.data:FlattenedDataset data="{chart03Data>/DataSet}">
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition name="Entity"
                                value="{chart03Data>Entity}" />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition name="{chart03Data>/Legend/0}"
                                value="{chart03Data>KF1}" />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>

                <viz:feeds>
                    <viz.feeds:FeedItem uid="size" type="Measure"
                        values="{chart03Data>/Legend}" />
                    <viz.feeds:FeedItem uid="color" type="Dimension"
                        values="Entity" />    
                </viz:feeds>
            </viz:VizFrame>
        </layout:flexContent>
      </layout:FixFlex>
</core:FragmentDefinition>