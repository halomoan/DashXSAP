<mvc:View
		xmlns="sap.uxap"
		xmlns:m="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:semantic="sap.m.semantic"
		xmlns:l="sap.ui.layout"
		xmlns:core="sap.ui.core"
		xmlns:tiles="sap.ui.dashxsap.view.menuView02Tiles"
		xmlns:vk="sap.ui.vk"
		xmlns:vbm="sap.ui.vbm" height="100%" displayBlock="true">

	<semantic:DetailPage
			navButtonPress="onNavBack"
			showNavButton="{detailView>/showNav}"
			title="{i18n>dashboardName}"
			busy="{detailView>/busy}"
			showFooter="{device>/system/desktop}"
			busyIndicatorDelay="{detailView>/delay}">

	
	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false" subSectionLayout="TitleOnLeft"
					  showTitleInHeaderContent="true" showHeaderContent="true">
		<headerTitle>
			<ObjectPageHeader objectImageURI="images/PPHGLogo.png"
							  objectImageShape=""
							  objectTitle="Pan Pacific Singapore"
							  objectSubtitle="Country: Singapore"
							  objectImageAlt="Hotel"
							  id="ObjectPageLayoutHeaderTitle"
							  isObjectIconAlwaysVisible="false"
							  isObjectTitleAlwaysVisible="false"
							  isObjectSubtitleAlwaysVisible="false"
							  isActionAreaAlwaysVisible="true"
							  showPlaceholder="true">
				<!--<navigationBar>
					<m:Bar>
						<m:contentLeft>
						
						</m:contentLeft>
						<m:contentMiddle>
							<m:Text text="Month To Date"/>
						</m:contentMiddle>
					</m:Bar>
				</navigationBar>
				-->
				<actions>
					
				</actions>
			</ObjectPageHeader>
		</headerTitle>
		<headerContent>
			<l:VerticalLayout>
				<m:ObjectStatus title="Total Rooms" text="346"/>
			</l:VerticalLayout>	
			<l:VerticalLayout>
				<m:ObjectStatus title="Occupied Rooms" text="330"/>
				<m:ObjectStatus title="AOR % Saleable Rooms" text="89%"/>
				<m:ObjectStatus title="ARR" text="209"/>
				<m:ObjectStatus title="F&amp;B Avg. Checks" text="63"/>
			</l:VerticalLayout>	
			<l:VerticalLayout>
				<m:Label text="Covers"/>
				<m:ObjectNumber number="221" numberUnit="UN"></m:ObjectNumber>	
			</l:VerticalLayout>
			<l:VerticalLayout>
				<m:Label text="Total Revenues"/>
				<m:NumericContent size="S" scale="M" value="1.1" valueColor="" class="sapUiTinyMargin" withMargin="false"/>
			</l:VerticalLayout>
			<l:VerticalLayout>
				<m:Label text=""/>
				<m:NumericContent scale="K" value="1.75" valueColor="Error" indicator="Down" press="press" class="sapUiTinyMargin" withMargin="false" Tooltips="AC"/>
			</l:VerticalLayout>
			
		
		</headerContent>
		<sections>
			<ObjectPageSection title="{i18n>overview}">
				<subSections>
					<ObjectPageSubSection title=" ">
						<blocks>
							<tiles:menuView02Tiles id="tilesBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
	</semantic:DetailPage>
</mvc:View>