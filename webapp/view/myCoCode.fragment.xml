<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core">
	
	<SelectDialog
		id="selectCoCode"
		noDataText="No Company Code Found"
		title="Select Company Code"
		search="searchCoCode"
		confirm="selectCoCode"
		close="selectCoCode"
		items="{
			path: '/MyCoCodes'
		}" >
		<StandardListItem
			title="{MyCoCodeID}"
			description="{ShortText}"
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>