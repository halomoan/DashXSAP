<mvc:View
		controllerName="sap.ui.dashxsap.controller.menuView02"
		xmlns="sap.uxap"
		xmlns:m="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:semantic="sap.m.semantic"
		xmlns:l="sap.ui.layout"
		xmlns:core="sap.ui.core"
		xmlns:sections="sap.ui.dashxsap.menuView02B"
			
		height="100%" displayBlock="true">

	<semantic:DetailPage
			navButtonPress="onNavBack"
			showNavButton="{detailView>/showNav}"
			title="{i18n>dashboardName}"
			busy="{detailView>/busy}"
			showFooter="{device>/system/desktop}"
			busyIndicatorDelay="{detailView>/delay}">

	
	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false" subSectionLayout="TitleOnLeft"
					  showTitleInHeaderContent="true" showHeaderContent="true">
		<headerTitle>
			<ObjectPageHeader objectImageURI="images/PPHGLogo.png"
							  objectImageShape=""
							  objectTitle="Pan Pacific Singapore"
							  objectSubtitle="Country: {headerData>/country}"
							  objectImageAlt="Hotel"
							  id="ObjectPageLayoutHeaderTitle"
							  isObjectIconAlwaysVisible="false"
							  isObjectTitleAlwaysVisible="false"
							  isObjectSubtitleAlwaysVisible="false"
							  isActionAreaAlwaysVisible="true"
							  showPlaceholder="true">
				<!--<navigationBar>
					<m:Bar>
						<m:contentLeft>
						
						</m:contentLeft>
						<m:contentMiddle>
							<m:Text text="Month To Date"/>
						</m:contentMiddle>
					</m:Bar>
				</navigationBar>
				-->
				<actions>
					
				</actions>
			</ObjectPageHeader>
		</headerTitle>
		<headerContent>
			<l:VerticalLayout>
				<m:Label text="{dataTiles>/dashxitemid}"></m:Label>
			</l:VerticalLayout>	
			
			<l:VerticalLayout>
				<m:Label text="Total Room"/>
				<m:ObjectNumber number="{headerData>/totalroom}" numberUnit="UN"></m:ObjectNumber>	
			</l:VerticalLayout>
			
			
		
		</headerContent>
		<sections>
			<ObjectPageSection title="{i18n>overview}">
				<subSections>
					<ObjectPageSubSection title=" ">
						<blocks>
							<sections:overview id="overviewBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="{i18n>room}">
				<subSections>
					<ObjectPageSubSection title=" ">
						<blocks>
							<sections:room id="roomBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
				<ObjectPageSection title="{i18n>rbe}">
				<subSections>
					<ObjectPageSubSection title=" ">
						<blocks>
							<sections:rbe id="rbeBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection title="{i18n>mod}">
				<subSections>
					<ObjectPageSubSection title=" ">
						<blocks>
							<sections:mod id="modBlock"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
	</semantic:DetailPage>
</mvc:View>