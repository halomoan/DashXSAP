<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Popover
		title="{detailView>text}"
		class="sapUiPopupWithPadding"
		placement="Right"
		state="Success"
		initialFocus="action">
		
		<List
					id="list"
					items="{detailView>/dcompares}" >
					<StandardListItem
						title="{detailView>text}"
						type="Active"
						press="onNavToCompare"
						iconDensityAware="false"
						iconInset="false" />
				</List>
		
	</Popover>
</core:FragmentDefinition>